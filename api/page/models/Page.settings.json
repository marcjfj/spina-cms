{
  "connection": "default",
  "collectionName": "pages",
  "info": {
    "name": "page",
    "description": "this is where the general page content goes"
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "paragraph": {
      "type": "text"
    },
    "name": {
      "type": "string"
    },
    "picture": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    }
  }
}